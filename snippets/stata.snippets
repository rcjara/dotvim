snippet fe
	foreach ${1:var} of local ${2:macro} {
		${3}
	}

snippet fv
	forvalues ${1:i} = ${2:1}/${3:endvalue} {
		${4}
	}

snippet prog
	capture program drop ${1:prog_name}
	program define ${2:prog_name}
	end

snippet log
	capture log close
	log using "logs/${1:file_name}.txt", text replace
	log close
